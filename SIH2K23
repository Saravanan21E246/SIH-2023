<script>
    function updateCountdown() {
        $.ajax({
            url: 'GetSchedule.php', // Update this with the correct PHP file to fetch schedule data
            type: 'GET',
            dataType: 'json',
            success: function(response) {
                const currentTime = new Date();

                // Convert schedule times to Date objects
                const onTime = new Date(currentTime.toDateString() + ' ' + response.from_time);
                const offTime = new Date(currentTime.toDateString() + ' ' + response.to_time);

                 const options = {
                      hour: 'numeric',
                      minute: 'numeric',
                      hour12: true
                  };

                  // Get the formatted time strings
                  const formattedOnTime = onTime.toLocaleTimeString('en-US', options);
                  const formattedOffTime = offTime.toLocaleTimeString('en-US', options);

                  $('#lightStart').text(formattedOnTime);
                  $('#lightOff').text(formattedOffTime);

                const dayStatusAsNumber = parseInt(response.dayStatus, 10);
               
// Add dayStatusAsNumber to offTime
                 offTime.setDate(offTime.getDate() + dayStatusAsNumber);
                  // console.log(offTime);
                // Check if the current time is within the schedule
                let timeRemaining;
                let alertMessage;

                if (currentTime >= onTime && currentTime < offTime) {
                    timeRemaining = new Date(offTime - currentTime);
                    alertMessage = 'Time To <span style="color: red;">OFF</span>';
                } else {
                    timeRemaining = new Date(onTime - currentTime);
                    alertMessage = 'Time To <span style="color: rgb(8, 218, 5);">ON</span>';
                }

                const hours = timeRemaining.getUTCHours();
                const minutes = timeRemaining.getUTCMinutes();
                const seconds = timeRemaining.getUTCSeconds();

                // Display the countdown and alert message
                document.getElementById('alert').innerHTML = alertMessage;
                document.getElementById('hour').innerHTML = `${hours}`;
                document.getElementById('minutes').innerHTML = `${minutes}`;
                document.getElementById('seconds').innerHTML = `${seconds}`;
            },
            error: function(xhr, status, error) {
                console.error(status + ": " + error);
            }
        });
    }

    // Initial call to update the countdown
    updateCountdown();

    // Update the countdown every second
    setInterval(updateCountdown, 1000);
</script>

 
<script>
 let previousState = "OFF"; // Initialize the previous state as OFF
  
function sendTelegramMessage(message) {
    // Your bot token and chat ID
    const botToken = "6105850338:AAGYG6hKQ31WktpYtKza86mxCSFeXZc4iY8";
    const chatId = "-1001920945805";
// Get the current date and time
const currentTime = new Date();

// Format the date with the month in text (e.g., "10/oct/2023")
const dateOptions = { day: "numeric", month: "short", year: "numeric" };
const formattedDate = currentTime.toLocaleDateString("en-US", dateOptions);

// Format the time (e.g., "12:34:56 PM")
const timeOptions = { hour: "2-digit", minute: "2-digit", second: "2-digit" };
const formattedTime = currentTime.toLocaleTimeString("en-US", timeOptions);

// Create the message text with the formatted date and time
const messageText = `*${message} - ${formattedDate} ${formattedTime}*`;

// Create the URL for the Telegram API
const apiUrl = `https://api.telegram.org/bot${botToken}/sendMessage`;

// Create a JSON payload with the message data
const data = {
  chat_id: chatId,
  text: messageText,
  parse_mode: "Markdown", // Specify that the message contains Markdown formatting
}

    // Send the message using the fetch API
    fetch(apiUrl, {
      method: "POST",
      headers: {
        "Content-Type": "application/json",
      },
      body: JSON.stringify(data),
    })
      .then((response) => response.json())
      .then((data) => {
        // Handle the response if needed
        console.log(data);
      })
      .catch((error) => {
        // Handle any errors
        console.error(error);
      });
  }

  
  
  function updateLED() {
      $.ajax({
          url: 'GetSchedule.php', // Replace with the correct path to your PHP file to fetch schedule data
          type: 'GET',
          dataType: 'json',
          success: function (response) {
              const currentTime = new Date();

              // Convert schedule times to Date objects
              const onTime = new Date(currentTime.toDateString() + ' ' + response.from_time);
              const offTime = new Date(currentTime.toDateString() + ' ' + response.to_time);
              const dayStatusAsNumber = parseInt(response.dayStatus, 10);

// Add dayStatusAsNumber to offTime
               offTime.setDate(offTime.getDate() + dayStatusAsNumber);
              // console.log(offTime);
              // Get the lightbulb icon element
              const lightbulbIcon = document.getElementById('lightbulb-icon21');

              // Check if the current time is within the specified on time and off time
              if (currentTime >= onTime && currentTime < offTime) {
                  // Turn the LED on (change the icon color to yellow)
                  lightbulbIcon.style.color = 'yellow';
                  document.getElementById('Status').innerHTML = `ON`;

                  // Send the Telegram message only if the state changes from OFF to ON
                  if (previousState === "OFF") {
                      sendTelegramMessage("ON ALL LIGHT");
                      setTimeout(turnOnSwitch1ON, 3000);
                      setTimeout(turnOnSwitch2ON, 5000);
                      setTimeout(turnOnSwitch3ON, 7000);
                      setTimeout(turnOnSwitch4ON, 9000);
                      previousState = "ON"; // Update the previous state
                  }
              } else {
                  // Turn the LED off (change the icon color to white)
                  lightbulbIcon.style.color = 'red';
                  document.getElementById('Status').innerHTML = `OFF`;

                  // Send the Telegram message only if the state changes from ON to OFF
                  if (previousState === "ON") {
                      sendTelegramMessage("OFF ALL LIGHT");
                      setTimeout(turnOnSwitch1OFF, 3000);
                      setTimeout(turnOnSwitch2OFF, 5000);
                      setTimeout(turnOnSwitch3OFF, 7000);
                      setTimeout(turnOnSwitch4OFF, 9000);
                      previousState = "OFF"; // Update the previous state
                  }
              }
          },
          error: function (xhr, status, error) {
              console.error(status + ": " + error);
          }
      });
  }

  // Initial call to update the LED state
  updateLED();

  // Update the LED state every minute
  setInterval(updateLED, 100); // Every 60,000 milliseconds (1 minute)
</script>
<script>
  function uploadTimes() {
      var fromTime = $("#fromTime").val();
      var toTime = $("#toTime").val();
      var dayStatus = document.getElementById('flexCheckDay').checked ? 1 : 0;

      $.ajax({
          type: "POST",
          url: "time.php",
          data: {
              fromTime: fromTime,
              toTime: toTime,
              dayStatus: dayStatus
          },
          success: function(response) {
              // Handle success, if needed
              console.log(response);
          },
          error: function(error) {
              // Handle error, if needed
              console.log(error);
          }
      });
  }
</script>

<script>
  $(document).ready(function() {
    setInterval(function() {
        $.ajax({
            url: 'update_progress.php',
            type: 'GET',
            dataType: 'json',
            success: function(data) {
                updateProgressBar('progressbar1', data.Lamp1_intensity);
                updateProgressBar('progressbar2', data.Lamp2_intensity);
                updateProgressBar('progressbar3', data.Lamp3_intensity);
                updateProgressBar('progressbar4', data.Lamp4_intensity);
            },
            error: function(error) {
                console.error('Error fetching data:', error);
            }
        });
    }, 1000);

    function updateProgressBar(barId, intensity) {
        var progressBar = $('#' + barId);
        var percentage = intensity * 25; // Assuming each intensity step represents 25%
        progressBar.width(percentage + '%');
        progressBar.text(percentage + '%');
        progressBar.attr('aria-valuenow', percentage);
    }
});

</script>

<script>
  document.getElementById('flexCheckDay').addEventListener('change', function () {
      var label = document.getElementById('dayLabel');
      label.innerText = this.checked ? 'All Day' : 'Today';
  });
</script>
