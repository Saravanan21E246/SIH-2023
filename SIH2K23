<script>
  function turnOnSwitch1ON() {
    var switchElement = document.getElementById("flexSwitchCheckChecked1");
    if (switchElement) {
      switchElement.checked = true;

      // Simulate a change event to trigger the functionality
      var event = new Event("change");
      switchElement.dispatchEvent(event);
    }
  }

  function turnOnSwitch1OFF() {
    var switchElement = document.getElementById("flexSwitchCheckChecked1");
    if (switchElement) {
      switchElement.checked = false;

      // Simulate a change event to trigger the functionality
      var event = new Event("change");
      switchElement.dispatchEvent(event);
    }
  }

  const toggleSwitch1 = document.getElementById("flexSwitchCheckChecked1");
  const lightBulbIcon1ON = document.getElementById("lightbulb-icon1");
  const lightBulbIcon1OFF = document.getElementById("lightbulb-icon1");

  toggleSwitch1.addEventListener("change", function() {
    if (this.checked) {
      lightBulbIcon1ON.classList.add("text-warning"); // Change color to yellow
      sendTelegramMessage("Lamp 1 ON");
    } else {
      lightBulbIcon1OFF.classList.remove("text-warning");
      sendTelegramMessage("Lamp 1 OFF");
    }
  });


</script>


