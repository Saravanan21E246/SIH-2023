   <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script> <!-- Include jQuery -->
   <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" >Mode</h5>
          
        </div>
        <div class="modal-body" id="exampleModalLabel">
          You are in Manual Mode....
          
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        </div>
      </div>
    </div>
  </div>
  <script>
    var exampleModal = document.getElementById('exampleModal');
  
    exampleModal.addEventListener('show.bs.modal', function (event) {
      // Button that triggered the modal
      var button = event.relatedTarget;
  
      // Extract info from data-bs-* attributes
      var recipient = button.getAttribute('data-bs-whatever');
  
      // Update the modal's content.
      var modalTitle = exampleModal.querySelector('.modal-title');
      var modalBody = exampleModal.querySelector('.modal-body');
  

      if(recipient==='Schedule'){
        modalTitle.textContent = 'Schedule';
        modalBody.innerHTML = 'Schedule Updated.....';
      }else{
        modalTitle.textContent = 'Mode';
      modalBody.innerHTML = 'You are in ' + recipient + ' mode....';

      }    
      // Automatically close the modal after 1 second
      setTimeout(function () {
        var modal = bootstrap.Modal.getInstance(exampleModal);
        modal.hide();
      }, 1500);
    });
  </script>

   <script>
    $(document).ready(function () {
      $("#manualButton").click(function () {
        updateDatabase(1, 0 , 0);
      });
  
      $("#automaticButton").click(function () {
        updateDatabase(0, 1 , 0);
      });

      $("#IntelligentButton").click(function () {
        updateDatabase(0, 0 , 1);
      });
  
      function updateDatabase(manualValue, automaticValue, IntelligentValue) {
        $.ajax({
          type: "POST",
          url: "action_mode.php",
          data: {manualValue: manualValue, automaticValue: automaticValue,IntelligentValue:IntelligentValue },
          success: function (response) {
            console.log(response);
            // You can handle the response from the server if needed
          },
          error: function (error) {
            console.error('Error updating database:', error);
          }
        });
      }
    });
</script>
  
  <script>
    $(document).ready(function () {
        function updateSwitchStatus() {
            // Make an Ajax request to get the manual value from the server
            $.ajax({
                type: "GET",
                url: "manual_mode.php", // Replace with the actual path to your PHP file
                dataType: "json",
                success: function (data) {
                    // Check the manual value and update the switch accordingly
                    if (data == 1) {
                        $("#flexSwitchCheckChecked1").prop("disabled", false);
                        $("#flexSwitchCheckChecked2").prop("disabled", false);
                        $("#flexSwitchCheckChecked3").prop("disabled", false);
                        $("#flexSwitchCheckChecked4").prop("disabled", false);
                        $("#flexSwitchCheckChecked5").prop("disabled", false);
                      
                        $("#customRange1").prop("disabled", false);
                        $("#customRange2").prop("disabled", false);
                        $("#customRange3").prop("disabled", false);
                        $("#customRange4").prop("disabled", false);
                        $("#customRange5").prop("disabled", false);
                    } else {
                        $("#flexSwitchCheckChecked1").prop("disabled", true);
                        $("#flexSwitchCheckChecked2").prop("disabled", true);
                        $("#flexSwitchCheckChecked3").prop("disabled", true);
                        $("#flexSwitchCheckChecked4").prop("disabled", true);
                        $("#flexSwitchCheckChecked5").prop("disabled", true);
                      
                        $("#customRange1").prop("disabled", true);
                        $("#customRange2").prop("disabled", true);
                        $("#customRange3").prop("disabled", true);
                        $("#customRange4").prop("disabled", true);
                        $("#customRange5").prop("disabled", true);
                    }
                },
                complete: function () {
                    // Schedule the next update after 1 second
                    setTimeout(updateSwitchStatus, 1000);
                }
            });
        }

        // Initial call to set the switch status
        updateSwitchStatus();
    });
</script>
  
<script>
  $(document).ready(function () {
    function updateText() {
        $.ajax({
            url: 'Readings.php',
            type: 'GET',
            dataType: 'json',
            success: function (response) {
                // Update Voltage values
                $('#Voltage1').text(response.Voltage.V1 + " V");

                // Update Current values
                $('#Current1').text(response.Current.C1);

                // Update Power values
                $('#Power1').text(response.Power.P1);

                // Update Energy values
                $('#Energy1').text(response.Energy.E1);

                // Update Environmental Intensity value
                $('#Environmental_intensity').text(response.LuxEnvironment);

                // Update lamp Intensity value
                $('#lamplux1').text(response.luxValue.lux1);

            },
            error: function (xhr, status, error) {
                console.error(status + ": " + error);
            }
        });
    }

    // Call the updateText function every 1000 milliseconds (1 second)
    setInterval(updateText, 1000);
});

</script>
<script>
  $(document).ready(function () {
    function updateText() {
        $.ajax({
            url: 'Node1Readings.php',
            type: 'GET',
            dataType: 'json',
            success: function (response) {
                // Update Voltage values
                $('#Voltage2').text(response.Voltage.V2 + " V");

                // Update Current values
                $('#Current2').text(response.Current.C2);

                // Update Power values
                $('#Power2').text(response.Power.P2);

                // Update Energy values
                $('#Energy2').text(response.Energy.E2);

                // Update lamp Intensity value
                $('#lamplux2').text(response.LuxEnvironment);

            },
            error: function (xhr, status, error) {
                console.error(status + ": " + error);
            }
        });
    }
    // Call the updateText function every 1000 milliseconds (1 second)
    setInterval(updateText, 1000);
});

</script>
   

<script>
  $(document).ready(function () {
    // Initial check and update
    updateManualIconColor();

    // Periodically check and update every 1 second
    setInterval(function () {
      updateManualIconColor();
    }, 1000);

    function updateManualIconColor() {
      $.ajax({
        type: "GET",
        url: "manual_mode.php",
        success: function (response) {
          // Update icon color based on the fetched value
          var manualIcon = $("#manualIcon");
          if (response == 1) {
            manualIcon.css("color", "green");
          } else {
            manualIcon.css("color", "red");
          }
        },
        error: function (error) {
          console.error('Error fetching manual value:', error.responseText);
        }
      });
    }
  });
</script>

