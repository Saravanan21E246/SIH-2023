<script>
  document.addEventListener('DOMContentLoaded', function () {
      var customRange1 = document.getElementById('customRange1');
      var intervalId = setInterval(updateCustomRange1, 1000);
      var userInteracted = false;

      function updateCustomRange1() {
          $.ajax({
              url: 'fetch_intensity1.php',
              type: 'GET',
              success: function (data) {
                  if (!userInteracted) { // Only update if the user hasn't interacted
                      customRange1.value = data;
                  }
              }
          });
      }

      $('#customRange1').on('input', function () {
          userInteracted = true;
          var rangeValue = $(this).val();

          $.ajax({
              type: 'POST',
              url: 'update_range1.php',
              data: { value: rangeValue },
              success: function (response) {
                  console.log(response);
              }
          });
      });
  });
</script>
 
