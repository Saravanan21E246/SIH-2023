<script>
    document.addEventListener('DOMContentLoaded', function () {
        // Get the checkbox element
        var checkbox = document.getElementById('flexSwitchCheckChecked1');
        var lightBulbIcon1 = document.getElementById('lightbulb-icon1');
        var lightBulbIcon11 = document.getElementById("lightbulb-icon11");        
        var lightBulbIconL1 = document.getElementById("lightbulb-iconL1");



        // Function to fetch Lamp1_switch value from the server
        function fetchSwitchState() {
            var xhr = new XMLHttpRequest();
            xhr.open('GET', 'getSwitchState1.php', true);
            xhr.onreadystatechange = function () {
                if (xhr.readyState == 4 && xhr.status == 200) {
                    // Update the checkbox state based on the fetched value
                    checkbox.checked = xhr.responseText.trim() === '1';

                    // Update the icon color based on the checkbox state
                    updateIconColor();
                }
            };

            // Send the request to the server
            xhr.send();
        }

        // Function to update the icon color based on the checkbox state
        function updateIconColor() {
            if (checkbox.checked) {
                lightBulbIcon1.classList.add('text-warning');             
                lightBulbIcon11.classList.add('text-warning');
                lightBulbIconL1.classList.add('text-warning');                          
            } else {
                lightBulbIcon1.classList.remove('text-warning');             
                lightBulbIcon11.classList.remove('text-warning');     
                lightBulbIconL1.classList.remove('text-warning');

            }
        }

        // Fetch Lamp1_switch value every 1 second
        var intervalId = setInterval(fetchSwitchState, 1000);

        // Listen for changes in the checkbox state
        $('#flexSwitchCheckChecked1').on('change', function () {
            clearInterval(intervalId); // Stop the automatic updates
            var isChecked = $(this).prop('checked') ? 1 : 0;

            // Send the state to the PHP script using AJAX
            $.ajax({
                type: 'POST',
                url: 'lamp1_control.php', // Replace with the path to your PHP script
                data: { state: isChecked },
                success: function (response) {
                    console.log(response); // You can handle the response from the PHP script here

                    // Update the icon color based on the checkbox state
                    updateIconColor();
                }
            });
        });
    });
</script>


