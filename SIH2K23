<script>
    // Bar Chart Configuration
    var ctx = document.getElementById("chart-bars").getContext("2d");
    var chartBars = new Chart(ctx, {
        type: "bar",
        data: {
            labels: [],
            datasets: [{
                label: "LuxEnvironment",
                tension: 0.4,
                borderWidth: 0,
                borderRadius: 4,
                borderSkipped: false,
                backgroundColor: "rgb(24,193,232)", // Sky Blue Background Color
                data: [],
                maxBarThickness: 6
            },
            {
                label: "LuxLamp1",
                tension: 0.4, 
                borderWidth: 0,
                borderRadius: 4,
                borderSkipped: false,
                backgroundColor: "rgb(152, 9, 160)", // Yellow Background Color for LuxLamp1
                data: [],
                maxBarThickness: 6
            },
            {
                label: "LuxLamp2",
                tension: 0.4,
                borderWidth: 0,
                borderRadius: 4,
                borderSkipped: false,
                backgroundColor: "rgb(9, 82, 160)", // Orange Background Color for LuxLamp2
                data: [],
                maxBarThickness: 6
            },
            {
                label: "LuxLamp3",
                tension: 0.4,
                borderWidth: 0,
                borderRadius: 4,
                borderSkipped: false,
                backgroundColor: "rgb(167, 2, 2)", // Orange Background Color for LuxLamp2
                data: [],
                maxBarThickness: 6
            }
          ],
        },
        options: {
            responsive: true,
            maintainAspectRatio: false,
            plugins: {
                legend: {
                    display: false,
                }
            },
            interaction: {
                intersect: false,
                mode: 'index',
            },
            scales: {
                y: {
                    grid: {
                        drawBorder: false,
                        display: false,
                        drawOnChartArea: false,
                        drawTicks: false,
                    },
                    ticks: {
                        display: true,
                        color: "#fff", // White color
                        padding: 15,
                        font: {
                            size: 14,
                            family: "Open Sans",
                            style: 'normal',
                            lineHeight: 2
                        },
                    },
                },
                x: {
                    grid: {
                        drawBorder: false,
                        display: false,
                        drawOnChartArea: false,
                        drawTicks: false
                    },
                    ticks: {
                        display: true,
                        color: "#fff", // White color
                    },
                },
            },
        },
    });

    // Line Chart Configuration
    var ctx2 = document.getElementById("chart-line").getContext("2d");
    var chartLine = new Chart(ctx2, {
        type: "line",
        data: {
            labels: [],
            datasets: [
                {
                    label: "Current1",
                    tension: 0.4,
                    borderWidth: 3,
                    pointRadius: 0,
                    borderColor: "rgb(152, 9, 160)",
                    backgroundColor: 'rgba(72,72,176,0)',
                    fill: true,
                    data: [],
                    maxBarThickness: 6
                },
                {
                    label: "Power1",
                    tension: 0.4,
                    borderWidth: 3,
                    pointRadius: 0,
                    borderColor: "#48BFCA",
                    backgroundColor: 'rgba(72,72,176,0)',
                    fill: true,
                    data: [],
                    maxBarThickness: 6
                },
            ],
        },
        options: {
            responsive: true,
            maintainAspectRatio: false,
            plugins: {
                legend: {
                    display: false,
                }
            },
            interaction: {
                intersect: false,
                mode: 'index',
            },
            scales: {
                y: {
                    grid: {
                        drawBorder: false,
                        display: true,
                        drawOnChartArea: true,
                        drawTicks: false,
                        borderDash: [5, 5]
                    },
                    ticks: {
                        display: true,
                        padding: 10,
                        color: '#b2b9bf',
                        font: {
                            size: 11,
                            family: "Open Sans",
                            style: 'normal',
                            lineHeight: 2
                        },
                    }
                },
                x: {
                    grid: {
                        drawBorder: false,
                        display: false,
                        drawOnChartArea: false,
                        drawTicks: false,
                        borderDash: [5, 5]
                    },
                    ticks: {
                        display: true,
                        color: '#b2b9bf',
                        padding: 20,
                        font: {
                            size: 11,
                            family: "Open Sans",
                            style: 'normal',
                            lineHeight: 2
                        },
                    }
                },
            },
        },
    });

    function fetchDataAndUpdateChartBars() {
        var xhr = new XMLHttpRequest();
        xhr.open('GET', 'LuxEnvironment.php', true);

        xhr.onload = function () {
            if (xhr.status >= 200 && xhr.status < 300) {
                var data = JSON.parse(xhr.responseText);

                // Update the bar chart data and labels
                chartBars.data.labels = data.labels;
                chartBars.data.datasets[0].data = data.datasets[0].data;
                chartBars.data.datasets[1].data = data.datasets[1].data; // LuxLamp1
                chartBars.data.datasets[2].data = data.datasets[2].data; // LuxLamp2
                chartBars.data.datasets[3].data = data.datasets[3].data; // LuxLamp2

                // Update the bar chart
                chartBars.update();
            }
        };

        xhr.send();
    }

    function fetchDataAndUpdateChartLine() {
        var xhr = new XMLHttpRequest();
        xhr.open('GET', 'LineChart.php', true);

        xhr.onload = function () {
            if (xhr.status >= 200 && xhr.status < 300) {
                var data = JSON.parse(xhr.responseText);

                // Update the line chart data
                chartLine.data.labels = data.labels;
                chartLine.data.datasets[0].data = data.datasets[0].data; // Current1
                chartLine.data.datasets[1].data = data.datasets[1].data; // Power1

                // Update the line chart
                chartLine.update();
            }
        };

        xhr.send();
    }

    // Fetch data and update charts every 1 second
    setInterval(fetchDataAndUpdateChartBars, 1000);
    setInterval(fetchDataAndUpdateChartLine, 1000);
</script>

